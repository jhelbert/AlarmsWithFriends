<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <TextView android:textAppearance="?android:attr/textAppearanceLarge" android:layout_height="wrap_content" android:id="@+id/textView3" android:layout_width="291dp" android:text="Settings"></TextView>
    <TextView android:id="@+id/textView1" android:text="TextView" android:layout_width="305dp" android:layout_height="wrap_content"></TextView>
    <SeekBar android:layout_height="wrap_content" android:max="15"
   android:progress="5" android:id="@+id/seekBar1" android:layout_width="match_parent"></SeekBar>
    <TextView android:text="TextView" android:id="@+id/textView2" android:layout_width="wrap_content" android:layout_height="wrap_content"></TextView>
    <SeekBar android:layout_height="wrap_content" android:max="4"
   android:progress="2" android:id="@+id/seekBar2" android:layout_width="match_parent"></SeekBar>
    <TextView android:textAppearance="?android:attr/textAppearanceMedium" android:layout_height="wrap_content" android:id="@+id/contact_disp" android:layout_width="wrap_content" android:text="Backup Contacts"></TextView>
    <Button android:onClick="doLaunchContactPicker" android:text="Button" android:id="@+id/button1" android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
    
    
    <ListView
  android:id="@+id/em_contacts" 
  android:layout_width="match_parent"
  android:layout_height="wrap_content"/>
</LinearLayout>
