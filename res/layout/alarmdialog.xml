<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/alarmsnooze" android:text="Snooze" android:layout_alignParentTop="true" android:layout_alignParentLeft="true"></Button>
    <Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/alarmdismiss" android:text="Dismiss" android:layout_alignParentTop="true" android:layout_toRightOf="@+id/alarmsnooze" android:layout_marginLeft="24dp"></Button>
    
</RelativeLayout>
